public class Product
    {
        private string name;
        private double price;
        private DateTime date;

        public Product(DateTime datte, string productName, double Newprice)
        {
            date = datte;
            name = productName;
            price = Newprice;
        }

        public string ProductName
        {
            get { return name; }
            set { ProductName = value; }
        }

        public double Price
        {
            get { return price; }
            set { price = value; }
        }
        public DateTime Date
        {
            get { return date; }
            set { date = value; }
        }

    }
